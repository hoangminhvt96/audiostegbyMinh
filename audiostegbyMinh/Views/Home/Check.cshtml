@model IEnumerable<audiostegbyMinh.Models.GoogleDriveFiles>
@{
    ViewBag.Title = "Kiểm tra watermark";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 style="text-decoration-color:#0bd325"><center>AUDIOTSTEG BY MINH</center></h2>
<script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
<center>
    <br />
    <div>
        <h2 style="color:#ff0000; background-color:#0bd325">CHECK WATERMARK FROM DOWNLOADED FILE</h2>
        <br />
        @using (Html.BeginForm("Checked", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <p>
                <label for="file">Vui lòng chọn file cần kiểm tra:</label>
                <br />
                <input type="file" name="file" id="file" />
                <br />
                <input class="btn-success" type="submit" value="KIỂM TRA" />
            </p>
        }
    </div>
</center>
   

